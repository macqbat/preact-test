{
  "name": "preact-widget-test",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "components/hello-world/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender --clean --template src/index.ejs --service-worker false",
    "serve": "npm run build && preact serve",
    "dev": "preact watch --template src/index.ejs",
    "dist": "microbundle --entry src/components/hello-world/index.js"
  },
  "keywords": [],
  "author": "Zouhir <zouhir@zouhir.org>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-preact": "^1.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^21.2.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "microbundle": "^0.6.0",
    "node-sass": "^4.12.0",
    "preact-cli": "^2.0.2",
    "preact-compat-enzyme": "^0.2.5",
    "preact-render-to-string": "^3.7.0",
    "preact-test-utils": "^0.1.4",
    "preact-widget-scripts": "^0.6.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "preact-context": "^1.1.3",
    "preact-habitat": "^3.1.0",
    "preact-redux": "^2.1.0",
    "preact-render-to-string": "^4.1.0",
    "preact-router": "^2.5.7",
    "react": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>src/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "^react$": "preact-compat-enzyme",
      "^react-dom$": "preact-compat-enzyme",
      "react-dom/server": "preact-render-to-string",
      "react-dom/test-utils": "preact-test-utils",
      "react-test-renderer/shallow": "preact-test-utils",
      "react-test-renderer": "preact-test-utils",
      "react-addons-test-utils": "preact-test-utils",
      "react-addons-transition-group": "preact-transition-group"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
